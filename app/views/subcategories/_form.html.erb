
<%= simple_form_for(@subcategory) do |f| %>

  <% @subcategory.errors.full_messages.each do |message| %>
  	<%= message %>
	<% end %>


  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.association :category %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
